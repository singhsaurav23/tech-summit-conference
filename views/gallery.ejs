<%- include("partials/header") %>

<div class="container">
   <header class="jumbotron">
   <div class="container">
   <h1>Welcome to TechSummit!</h1>
	<p>View our<strong id="year"> hand-picked</strong> photos</p>
</div>
</header>
<div class="row text-center" style="display:flex; flex-wrap: wrap;">
		<% gallery.forEach(function(pic){ %>
<div class="col-md-3 col-sm-6">
	<div class="thumbnail">
		<img src="<%= pic.url %>">
	<div class="caption">
		<h4><%= pic.title %></h4>
		</div>
	</div>
</div>	
<% }); %>
</div>
</div>
<%- include("partials/footer") %>